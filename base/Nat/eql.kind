// Confirms if two natural numbers (a, b) are equal
// - Nat.eql(5, 6) = false
// - Nat.eql(3, 3) = true

Nat.eql(a: Nat, b: Nat): Bool
  case a {
    zero: case b {
      zero: Bool.true
      succ: Bool.false
    }
    succ: case b {
      zero: Bool.false
      succ: Nat.eql(a.pred, b.pred)
    }
  }
